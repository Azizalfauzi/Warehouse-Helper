<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/onboardingSatu"
        android:name="org.d3ifcool.warehousehelper.boading.OnboardingSatu"
        android:label="fragment_onboarding_satu"
        tools:layout="@layout/fragment_onboarding_satu" >
        <action
            android:id="@+id/action_onboardingSatu_to_onboardingDua"
            app:destination="@id/onboardingDua" />
        <action
            android:id="@+id/action_onboardingSatu_to_loginActivity"
            app:destination="@id/loginActivity" />
    </fragment>
    <fragment
        android:id="@+id/onboardingDua"
        android:name="org.d3ifcool.warehousehelper.boading.OnboardingDua"
        android:label="fragment_onboarding_dua"
        tools:layout="@layout/fragment_onboarding_dua" >
        <action
            android:id="@+id/action_onboardingDua_to_onboardingTiga"
            app:destination="@id/onboardingTiga" />
    </fragment>
    <fragment
        android:id="@+id/onboardingTiga"
        android:name="org.d3ifcool.warehousehelper.boading.OnboardingTiga"
        android:label="fragment_onboarding_tiga"
        tools:layout="@layout/fragment_onboarding_tiga" >
        <action
            android:id="@+id/action_onboardingTiga_to_loginActivity"
            app:destination="@id/loginActivity" />
    </fragment>
    <activity
        android:id="@+id/loginActivity"
        android:name="org.d3ifcool.warehousehelper.autentifikasi.LoginActivity"
        android:label="activity_login"
        tools:layout="@layout/activity_login" />
    <activity
        android:id="@+id/aboutActivity"
        android:name="org.d3ifcool.warehousehelper.AboutActivity"
        android:label="activity_about"
        tools:layout="@layout/activity_about" />
    <activity
        android:id="@+id/signUpActivity"
        android:name="org.d3ifcool.warehousehelper.autentifikasi.SignUpActivity"
        android:label="activity_sign_up"
        tools:layout="@layout/activity_sign_up" />
    <activity
        android:id="@+id/dashboardActivity"
        android:name="org.d3ifcool.warehousehelper.DashboardActivity"
        android:label="activity_dashboard"
        tools:layout="@layout/activity_dashboard" />
    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="org.d3ifcool.warehousehelper.SplashScreenFragment"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_onboardingSatu"
            app:destination="@id/onboardingSatu" />
    </fragment>
</navigation>